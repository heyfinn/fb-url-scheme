{
  "manifest_version": 2,

  "name": "URL Scheme of Facebook",
  "description": "Give you Facebook protocol based urls.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "notifications",
    "clipboardWrite",
    "clipboardRead",
    "http://*/",
    "https://*/"
  ]
}
